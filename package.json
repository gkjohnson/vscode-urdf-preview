{
    "name": "urdf-viewer",
    "displayName": "URDF Viewer",
    "description": "",
    "version": "0.0.1",
    "publisher": "gkjohnson",
    "author": "Garrett Johnson <garrett.kjohnson@gmail.com>",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:urdf-viewer.previewURDF",
        "onLanguage:xml"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "urdf-viewer.previewURDF",
                "title": "Preview URDF Model",
                "icon": {
                    "dark": "./resources/icons/google-material-3d_rotation-dark.svg",
                    "light": "./resources/icons/google-material-3d_rotation-light.svg"
                }
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "urdf-viewer.previewURDF",
                    "when": "urdfActive",
                    "group": "navigation"
                }
            ],
            "editor/title/context": [
                {
                    "command": "urdf-viewer.previewURDF",
                    "when": "urdfActive",
                    "group": "urdf"
                }
            ]
        },
        "keybindings": [
            {
                "command": "urdf-viewer.previewURDF",
                "key": "alt+r",
                "mac": "alt+r",
                "when": "urdfActive"
            }
        ],
        "languages": [
            {
                "id": "xml",
                "extensions": [
                    ".urdf"
                ]
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "eslint": "^4.11.0",
        "urdf-loader": "^0.3.1"
    },
    "dependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "three": "^0.94.0",
        "threejs-model-loader": "0.0.5",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    }
}
